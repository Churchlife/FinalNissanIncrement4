
@{
    ViewBag.Title = "Index";
}

@model IEnumerable<NissanCartTest01.WebUi.Models.Genre>
@{
    ViewBag.Title = "Store";
}
<h3>Browse Vehicle Categories</h3>

<p>
    Select from @Model.Count() genres:
</p>
<ul>
    @foreach (var genre in Model)
    {
        <li>@Html.ActionLink(genre.Name, "Browse", new { genre = genre.Name })</li>
    }
</ul>